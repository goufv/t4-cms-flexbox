eq value \"button\"}}\r\n            <div class=\"button-container\">\r\n              <a href=\"{{publish element=\"ct_itemLinkURL\"}}\" class=\"button-flex\" role=\"button\">\r\n                {{publish element=\"ct_itemLinkText\"}}\r\n              </a>\r\n            </div>\r\n          {{/eq}}\r\n          {{#eq value \"hyperlink\"}}\r\n            <a href=\"{{publish element=\"ct_itemLinkURL\"}}\" class=\"link\">\r\n              {{publish element=\"ct_itemLinkText\"}} Â»\r\n            </a>\r\n          {{/eq}}\r\n        {{/if}}\r\n      {{/each}}\r\n    </div>\r\n  </div>\r\n</div>\r\n","m_oFile":null,"m_fIsFile":false}],"elements":{"extension":9,"format processor":7,"formatcode":12,"name":10,"syntax":11,"templateid":8},"elementsByAlias":{"extension":9,"format processor":7,"formatcode":12,"name":10,"syntax":11,"templateid":8},"language":"en"}